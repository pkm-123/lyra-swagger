{
  "components": {
    "responses": {
      "Errors": {
        "description": "Standard error response.",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Errors"
            }
          }
        }
      },
      "NoContent": {
        "description": "The request is successful but no data from server is expected."
      }
    },
    "schemas": {
      "Any": {
        "description": "A type similar to TypeScript's `any`.",
        "anyOf": [
          {
            "type": "boolean",
            "nullable": true
          },
          {
            "type": "integer",
            "nullable": true
          },
          {
            "type": "number",
            "nullable": true
          },
          {
            "type": "string",
            "nullable": true
          },
          {
            "type": "array",
            "nullable": true
          },
          {
            "type": "object",
            "additionalProperties": true,
            "nullable": true
          }
        ]
      },
      "CommonResponse": {
        "type": "object",
        "description": "Common response structure for holding warnings and errors.",
        "properties": {
          "errors": {
            "type": "array",
            "description": "List of errors.",
            "items": {
              "$ref": "#/components/schemas/Error"
            }
          },
          "warnings": {
            "type": "array",
            "description": "List of warnings.",
            "items": {
              "$ref": "#/components/schemas/Warning"
            }
          }
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "code": {
            "description": "Error code. Each error code has a unique meaning.",
            "type": "number",
            "externalDocs": {
              "url": "https://dev.azure.com/mabinnovation/MAB-Digital/_workitems/edit/20036/"
            }
          },
          "message": {
            "description": "Error description for developer. Should not be shown to end-user directly.",
            "type": "string"
          },
          "details": {
            "description": "Data that can include details of what causes the error. Does not guarantee to be present.",
            "properties": {
              "fields": {
                "type": "array",
                "items": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string"
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "required": ["code", "message"]
      },
      "Errors": {
        "type": "object",
        "properties": {
          "errors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Error"
            }
          }
        },
        "required": ["errors"]
      },
      "Warning": {
        "description": "Warning object. Made an alias here in case we need to add some other properties.",
        "allOf": [
          {
            "$ref": "#/components/schemas/Error"
          }
        ]
      },
      "WithPaginationToken": {
        "type": "object",
        "properties": {
          "paginationToken": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "HeaderApimSubscriptionKey": {
        "description": "Subscription key is required in header to access the API.",
        "type": "apiKey",
        "in": "header",
        "name": "Ocp-Apim-Subscription-Key"
      },
      "HeaderBearerToken": {
        "description": "Bearer token is required in header to access the API.",
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      },
      "HeaderVersion": {
        "description": "Version is required in header to access the API.",
        "type": "apiKey",
        "in": "header",
        "name": "Api-Version"
      }
    }
  }
}
